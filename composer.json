{
  "name": "tomodomo/block-starter",
  "description": "A template for building a new Gutenberg block",
  "type": "wordpress-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Tomodomo",
      "email": "hello@tomodomo.co"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "installer-name": "tomodomo-block-starter"
  },
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
    "sirbrillig/phpcs-variable-analysis": "^2.0",
    "squizlabs/php_codesniffer": "^3.2"
  }
}

